{
  "manifest_version": 3,
  "name": "Full width of ChatGPT for Google",
  "description": "右侧宽度加到700px, 岂不美哉~",
  "version": "1.0",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "128": "icon128.png"
  },
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
}
